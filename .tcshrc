alias vim./ 'vim ./'
alias v. 'vim ./'
alias v./ 'vim ./'
alias vi. 'vim ./'
alias vi./ 'vim ./'
alias vim- 'vim -'
alias vi- 'vim -'
alias make 'set make_cmp = `cat Makefile | sed -n '\''s/:$//p'\'' | xargs` && complete make "C/*/($make_cmp)/" && \make'
alias maek 'make'
alias amek 'make'
alias amke 'make'
alias meak 'make'
alias meka 'make'

alias l 'ls'
alias v 'vim'
alias vi 'vim'
alias ci 'vim'
alias cim 'vim'
alias in 'vim'
alias vin 'vim'
alias cin 'vim'
alias vm 'vim'
alias t 'tmux'
alias ta 'tmux a -t'
alias taa 'tmux a'
alias tls 'tmux ls | nl'
alias tlsa 'tmux a -t `tmux ls | sed '\''s/:.*$//g'\'' | tail -n +\!* | head -1`'
alias tn 'tmux new -s'
alias tkill 'tmux kill-session -t'
alias tkillall 'tmux kill-server'
alias git 'set git_cmp = `\git log | grep "^commit" | cut -d" " -f2 | cut -c 1-10 | xargs` && complete gvdiff "C/*/($git_cmp)/" && \git'
alias g 'git'
alias gi 'git'
alias gst 'git status'
