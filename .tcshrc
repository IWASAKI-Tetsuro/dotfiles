alias cim 'vim'
alias in 'vim'
alias vin 'vim'
alias cin 'vim'
alias t 'tmux'
alias ta 'tmux a -t'
alias taa 'tmux a'
alias tls 'tmux ls | nl'
alias tlsa 'tmux a -t `tmux ls | sed '\''s/:.*$//g'\'' | tail -n +\!* | head -1`'
