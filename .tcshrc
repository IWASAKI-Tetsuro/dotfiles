# complete
set filec
set autolist=ambiguous
set complete=enhance
set correct=all
set autocorrect
set autoexpand
set listjobs=long

# history
if ( ! -e ~/.tcsh_history) then
  touch ~/.tcsh_history
endif
set histfile = ~/.tcsh_history
set history = 10000
set savehist = (10000 merge)
set histdup = erase

# other
set color
set host=`hostname`
set nobeep
set noclobber
set notify
set path = ($path ~/.fzf/bin ~/.local/bin)
set rmstar
set tperiod=1

alias gitbranch 'git branch --no-color |& sed -e "/^[^*]/d" -e "s/* \(.*\)/\[\1\]/"'
alias precmd 'history -S && history -L'
set prompt = "%{\e[32m%}%n@%m %{\e[01;34m%}[%P] %{\e[0m%}[%~] %{\e[32m%}`gitbranch` \n%{\e[01;34m%}-> %%%{\e[0m%} "
alias cd 'chdir \!* && pwd -L >> ~/.cd_history && set prompt = "%{\e[32m%}%n@%m %{\e[01;34m%}[%P] %{\e[0m%}[%~] %{\e[32m%}`gitbranch` \n%{\e[01;34m%}-> %%%{\e[0m%} "'

if ( ! -e ~/.cd_history) then
  touch ~/.cd_history
endif

alias dclean 'awk '\''{ seen[$0] = NR } END { for (line in seen) print seen[line], line }'\'' ~/.cd_history | sort -k1,1n | cut -d" " -f2- > ~/.tmp/.cd_history.temp && mv ~/.tmp/.cd_history.temp ~/.cd_history'
alias dhist 'dclean && cat -n ~/.cd_history | tail -n -20'
alias dgrep 'dclean && cat -n ~/.cd_history | grep'
alias dcd 'cd `tail -n +\!* ~/.cd_history | head -1` && pwd -L'
alias dcd- 'dclean && cd `tail -n -1 ~/.cd_history ` && pwd -L'
alias drm 'sed -i '\''\!*d'\'' ~/.cd_history'
alias dless 'dclean && cat -n ~/.cd_history | less'
alias dhis dhist
alias dhi dhist
alias dls dhist
alias dles dless
alias cdhistory 'dclean && vim ~/.cd_history'
alias vimrc 'vim ~/.vimrc'
alias tcshrc 'vim ~/.tcshrc'
alias gitconfig 'vim ~/.gitconfig'
alias tmuxconf 'vim ~/.tmux.conf'

alias reload 'source ~/.tcshrc'
alias reloa 'source ~/.tcshrc'
alias relo 'source ~/.tcshrc'
alias ls 'ls -F --color=auto --group-directories-first'
alias ll 'ls -vhl --time-style=+"%Y/%m/%d %H:%M:%S"'
alias la 'll -a'
alias llt 'ls -thl --time-style=+"%Y/%m/%d %H:%M:%S"'
alias lat 'llt -a'
alias tree 'tree -C'
alias grep 'grep --color=auto'
alias fgrep 'fgrep --color=auto'
alias egrep 'egrep --color=auto'

